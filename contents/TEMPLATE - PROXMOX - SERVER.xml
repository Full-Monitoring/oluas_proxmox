                            <path>$.vmid</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
                <discovery_rule>
                    <name>Proxmox Discovery - STORAGE</name>
                    <type>DEPENDENT</type>
                    <key>ProxmoxDiscoveryStorage</key>
                    <delay>0</delay>
                    <lifetime>0d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#STORAGE} DISK</name>
                            <type>DEPENDENT</type>
                            <key>storage.disk[{#STORAGE}]</key>
                            <delay>0</delay>
                            <units>b</units>
                            <applications>
                                <application>
                                    <name>proxmox</name>
                                </application>
                                <application>
                                    <name>storage</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.[?(@.storage == '{#STORAGE}')].disk.first()</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>getProxmoxDataStorage</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#STORAGE} MAXDISK</name>
                            <type>DEPENDENT</type>
                            <key>storage.maxdisk[{#STORAGE}]</key>
                            <delay>0</delay>
                            <units>b</units>
                            <applications>
                                <application>
                                    <name>proxmox</name>
                                </application>
                                <application>
                                    <name>storage</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.[?(@.storage == '{#STORAGE}')].maxdisk.first()</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>getProxmoxDataStorage</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#STORAGE} DISK PERCENT</name>
                            <type>CALCULATED</type>
                            <key>storage.percent.disk[{#STORAGE}]</key>
                            <delay>3m</delay>
                            <units>%</units>
                            <params>100*last(&quot;storage.disk[{#STORAGE}]&quot;)/last(&quot;storage.maxdisk[{#STORAGE}]&quot;)</params>
                            <applications>
                                <application>
                                    <name>proxmox</name>
                                </application>
                                <application>
                                    <name>storage</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>getProxmoxDataStorage</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#STORAGE}</lld_macro>
                            <path>$.storage</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$LOGIN_PASS}</macro>
                    <type>SECRET_TEXT</type>
                    <description>senha do usuario</description>
                </macro>
                <macro>
                    <macro>{$LOGIN_USER}</macro>
                    <value>root@pam</value>
                    <description>usuario com acesso as informacoes do proxmox</description>
                </macro>
                <macro>
                    <macro>{$SCRIPT_PY}</macro>
                    <value>python3 /home/script/py/proxmox.py</value>
                    <description>python3 + localização do script python da api</description>
                </macro>
                <macro>
                    <macro>{$SERVER_IP}</macro>
                    <value>127.0.0.1</value>
                    <description>ip do servidor proxmox</description>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
